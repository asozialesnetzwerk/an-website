// dprint-ignore-file
{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "checkJs": false,
    "noEmit": true,
    "plugins": [
      {"name": "typescript-eslint-language-service"}
    ],
    "alwaysStrict": false,
    "noUnusedLocals": false,
    "lib": ["esnext", "dom", "dom.iterable"],
    "moduleResolution": "bundler",
    "verbatimModuleSyntax": true,
    "types": []
  }
}
